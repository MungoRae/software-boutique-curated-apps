{
    "listed": true,
    "name": "Git-Cola",
    "summary": "Graphical interface for the Git version control system.",
    "developer-name": "David Aguilar and Contributors",
    "developer-url": "https://github.com/git-cola/git-cola/graphs/contributors",
    "description": "A graphical interface to git, optimized for working with the git index. Git-cola can compare arbitrary commits using standard merge tools. With it you can search commit messages, content, authors, paths, data ranges etc. and it makes it easy to interactively edit the index. ",
    "launch-cmd": "git-cola",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "https://git-cola.github.io/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "git-cola",
            "install-packages": [
                "git",
                "git-cola"
            ],
            "remove-packages": [
                "git-cola"
            ],
            "source": "universe"
        }
    }
}
