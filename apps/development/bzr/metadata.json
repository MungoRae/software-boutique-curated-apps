{
    "listed": true,
    "name": "Bazaar Explorer",
    "summary": "An interface for the Bazaar version control system",
    "developer-name": "Canonical",
    "developer-url": "https://www.canonical.com/",
    "description": "Provides an easy-to-use interface to the Bazaar version control system. It is designed to be simple enough for casual users of version control, yet powerful enough for experts to be productive. ",
    "tags": "Version Control",
    "launch-cmd": "bzr explorer",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "http://doc.bazaar.canonical.com/explorer/en/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "bzr-explorer",
            "install-packages": [
                "bzr",
                "bzr-explorer"
            ],
            "remove-packages": [
                "bzr",
                "bzr-explorer"
            ],
            "source": "main"
        }
    }
}
