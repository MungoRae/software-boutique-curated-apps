    "alternate-to": null,
    "urls": {
        "info": "https://github.com/MrS0m30n3/youtube-dl-gui",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "youtube-dlg",
            "install-packages": [
                "youtube-dl",
                "youtube-dlg"
            ],
            "remove-packages": [
                "youtube-dl",
                "youtube-dlg"
            ],
            "source": "ppa:flexiondotorg/youtube-dl-gui"
        }
    }
}
