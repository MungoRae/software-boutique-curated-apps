{
    "listed": true,
    "name": "Pithos",
    "summary": "Native Pandora radio client",
    "developer-name": "Various Contributors",
    "developer-url": "https://github.com/pithos/pithos/graphs/contributors",
    "description": "A native <a onclick=\"cmd('link?http://www.pandora.com')\">Pandora Radio</a> client for Linux. It's more lightweight than the Pandora.com web client, and integrates with desktop features such as media keys, notifications, and the sound menu. <u>Currently Pandora Radio is only available in U.S., Australia and New Zealand.</u> ",
    "launch-cmd": "pithos",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "http://pithos.github.io/",
        "android-app": "https://play.google.com/store/apps/details?id=com.pandora.android",
        "ios-app": "https://itunes.apple.com/en/app/pandora-radio/id284035177"
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "pithos",
            "install-packages": [
                "pithos"
            ],
            "remove-packages": [
                "pithos"
            ],
            "source": "universe"
        }
    }
}
