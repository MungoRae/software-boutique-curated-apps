{
    "listed": true,
    "name": "Sound Converter",
    "summary": "Converts audio files to another format",
    "developer-name": "Various Contributors",
    "developer-url": "https://github.com/kassoulet/soundconverter/blob/py3k/AUTHORS",
    "description": "SoundConverter is a simple sound converter application It reads sound files in any format supported by GStreamer and outputs them in Ogg Vorbis, FLAC, or WAV format, or MP3 format. ",
    "tags": "Media Conversion",
    "launch-cmd": "soundconverter",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "http://soundconverter.org",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "arm64",
        "armhf"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "soundconverter",
            "install-packages": [
                "soundconverter",
                "gstreamer1.0-plugins-ugly"
            ],
            "remove-packages": [
                "soundconverter"
            ],
            "source": "universe"
        }
    }
}
