{
    "listed": true,
    "name": "Asunder",
    "summary": "Audio CD ripper and encoder",
    "developer-name": "Various Contributors",
    "developer-url": "http://littlesvr.ca/asunder/credits.php",
    "description": "Asunder can be used to save tracks from Audio CDs. Supports WAV, MP3, Ogg Vorbis, FLAC, and Wavpack audio files. Uses CDDB to name and tag each track. Can encode to multiple formats in one session. Creates M3U playlists. Allows for each track to be by a different artist. ",
    "launch-cmd": "asunder",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "http://www.littlesvr.ca/asunder/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "methods": ["apt"],
    "apt": {
        "default": {
            "main-package": "asunder",
            "install-packages": [
                "asunder",
                "lame",
                "flac",
                "wavpack"
            ],
            "remove-packages": [
                "asunder"
            ],
            "source": "universe"
        }
    },
    "snap": {
		"name": null
	}
}
