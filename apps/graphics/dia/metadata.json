{
    "listed": true,
    "name": "Dia",
    "summary": "Diagrams, graphs and charts editor",
    "developer-name": "GNOME Team",
    "developer-url": "https://www.gnome.org/about/",
    "description": "An editor for diagrams, graphs, charts etc. There is support for UML static structure diagrams (class diagrams), Entity-Relationship diagrams, network diagrams and much more. Diagrams can be exported to postscript and many other formats. ",
    "launch-cmd": "dia-gnome",
    "proprietary": false,
    "alternate-to": "Microsoft&reg; Office Visio&reg;",
    "urls": {
        "info": "https://wiki.gnome.org/action/show/Apps/Dia",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "methods": ["apt"],
    "apt": {
        "default": {
            "main-package": "dia-gnome",
            "install-packages": [
                "dia-gnome"
            ],
            "remove-packages": [
                "dia-gnome"
            ],
            "source": "universe"
        }
    },
    "snap": {
		"name": null
	}
}
