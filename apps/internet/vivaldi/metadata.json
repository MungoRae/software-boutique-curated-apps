{
    "listed": true,
    "name": "Vivaldi",
    "summary": "Web browser built with power users in mind",
    "developer-name": "Vivaldi Technologies",
    "developer-url": "https://vivaldi.com/",
    "description": "Vivaldi browser is made with power users in mind by people who love the Web. ",
    "launch-cmd": "vivaldi",
    "proprietary": true,
    "alternate-to": null,
    "urls": {
        "info": "https://vivaldi.net",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "methods": ["apt"],
    "apt": {
        "default": {
            "main-package": "vivaldi-stable",
            "install-packages": [
                "vivaldi-stable"
            ],
            "remove-packages": [
                "vivaldi-stable"
            ],
            "source": "manual",
            "list-file": "vivaldi.list",
            "list-key-url": "http://repo.vivaldi.com/stable/linux_signing_key.pub",
            "list-contents": "deb http://repo.vivaldi.com/archive/deb/ stable main"
        }
    },
    "snap": {
		"name": null
	}
}
