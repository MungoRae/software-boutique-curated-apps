{
    "listed": true,
    "name": "Remmina",
    "summary": "Remote desktop connect client with support for RDP and VNC",
    "developer-name": "Various Contributors",
    "developer-url": "https://github.com/FreeRDP/Remmina/blob/next/AUTHORS",
    "description": "A remote desktop connection client able to display and control a remote desktop session. It supports multiple network protocols in an integrated and consistent user interface. Currently RDP, VNC, NX, XDMCP and SSH protocols are supported. ",
    "launch-cmd": "remmina",
    "proprietary": false,
    "alternate-to": "Microsoft&reg; Remote Desktop",
    "urls": {
        "info": "http://remmina.sourceforge.net/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "remmina",
            "install-packages": [
                "remmina",
                "remmina-plugin-rdp",
                "remmina-plugin-vnc",
                "remmina-plugin-nx",
                "remmina-plugin-xdmcp"
            ],
            "remove-packages": [
                "remmina",
                "remmina-plugin-rdp",
                "remmina-plugin-vnc",
                "remmina-plugin-nx",
                "remmina-plugin-xdmcp"
            ],
            "source": "main"
        }
    }
}
