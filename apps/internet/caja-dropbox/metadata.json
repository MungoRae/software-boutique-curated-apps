{
    "listed": true,
    "name": "Caja Dropbox",
    "summary": "Add dropbox integration to the Caja File Manager",
    "developer-name": "MATE Team",
    "developer-url": "https://github.com/mate-desktop/caja-dropbox/graphs/contributors",
    "description": "Lets you bring your photos, documents, and videos anywhere into Dropbox's cloud and optionally share them with others easily. ",
    "tags": "Cloud and Sync",
    "launch-cmd": "caja-dropbox",
