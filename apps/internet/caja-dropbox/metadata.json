        "android-app": "https://play.google.com/store/apps/details?id=com.dropbox.android",
        "ios-app": "https://itunes.apple.com/us/app/dropbox/id327630330"
    },
    "arch": [
        "i386",
        "amd64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
