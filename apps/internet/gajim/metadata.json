{
    "listed": true,
    "name": "Gajim",
    "summary": "Jabber client",
    "developer-name": "Gajim Team",
    "developer-url": "https://dev.gajim.org/gajim/gajim/project_members",
    "description": "A Jabber client with a tabbed user interface for normal chats, group chats, and has many features such as, TLS, GPG, SSL, multiple accounts, avatars, file transfers, audio/video call, D-Bus and Metacontacts. ",
    "launch-cmd": "gajim",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "https://gajim.org/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "gajim",
            "install-packages": [
                "gajim",
                "python-appindicator"
            ],
            "remove-packages": [
                "gajim"
            ],
            "source": "universe"
        }
    }
}
