{
    "listed": true,
    "name": "Wireshark",
    "summary": "Network protocol analyzer",
    "developer-name": "Various Contributors",
    "developer-url": "https://www.wireshark.org/about.html",
    "description": "The world's foremost network protocol analyzer. It lets you see what's happening on your network at a microscopic level. It is the de facto (and often de jure) standard across many industries and educational institutions. ",
    "launch-cmd": "wireshark-gtk",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "https://www.wireshark.org/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "artful": {
            "main-package": "wireshark-gtk",
            "install-packages": [
                "wireshark-gtk"
            ],
            "remove-packages": [
                "wireshark-gtk",
                "wireshark-common"
            ],
            "source": "universe"
        },
        "all": {
            "main-package": "wireshark-gtk",
            "install-packages": [
                "wireshark-gtk"
            ],
            "remove-packages": [
                "wireshark-gtk",
                "wireshark-common"
            ],
            "source": "ppa:wireshark-dev/stable"
        }
    }
}
