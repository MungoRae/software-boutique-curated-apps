{
    "listed": true,
    "name": "D\u00e9j\u00e0 Dup",
    "summary": "A simple backup tool",
    "developer-name": "Various Contributors",
    "developer-url": "https://git.launchpad.net/deja-dup/tree/AUTHORS",
    "description": "D\u00e9j\u00e0 Dup is a simple backup tool. It hides the complexity of backing up the Right Way (encrypted, off-site, and regular) and uses duplicity as the backend. ",
    "tags": "Maintenance, Backups",
    "launch-cmd": "deja-dup",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "https://launchpad.net/deja-dup",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "arm64",
        "armhf"
    ],
    "releases": [
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "deja-dup",
            "install-packages": [
                "deja-dup",
                "deja-dup-backend-cloudfiles",
                "deja-dup-backend-gvfs",
                "deja-dup-backend-s3",
                "deja-dup-caja"
            ],
            "remove-packages": [
                "deja-dup",
                "deja-dup-backend-cloudfiles",
                "deja-dup-backend-gvfs",
                "deja-dup-backend-s3",
                "deja-dup-caja"
            ],
            "source": "main"
        }
    }
}
