    "alternate-to": "Microsoft&reg; Outlook",
    "urls": {
        "info": "https://wiki.gnome.org/Apps/Evolution",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "methods": ["apt"],
    "apt": {
        "default": {
            "main-package": "evolution",
            "install-packages": [
                "evolution",
                "evolution-ews",
                "evolution-plugins"
            ],
