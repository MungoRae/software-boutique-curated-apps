{
    "listed": true,
    "name": "Caja Share",
    "summary": "Extension for the file manager to create shares.",
    "developer-name": "MATE Desktop Team",
    "developer-url": "https://mate-desktop.org",
    "description": "An extension that allows you to quickly share a folder from the Caja file manager. It uses Samba, so your folders can be accessed by any operating system.",
    "launch-cmd": null,
    "proprietary": false,
    "alternate-to": "Windows&reg; Simple File Sharing",
    "urls": {
        "info": "https://www.samba.org/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "methods": ["apt"],
    "apt": {
        "default": {
            "main-package": "caja-share",
            "install-packages": [
                "caja-share",
                "samba"
            ],
            "remove-packages": [
                "caja-share",
                "samba"
            ],
            "source": "universe"
        }
    },
    "snap": {
		"name": null
	}
}
