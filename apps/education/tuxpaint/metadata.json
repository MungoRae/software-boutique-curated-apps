{
    "listed": true,
    "name": "Tux Paint",
    "summary": "Creative drawing tool aimed at children.",
    "developer-name": "Bill Kendrick and Contributors",
    "developer-url": "http://www.tuxpaint.org/developers/",
    "description": "An award-winning drawing program designed for children aged 3 to 12. It provides an easy to use interface, fun sound effects and Tux as the cartoon mascot to guide the children throughout the program. ",
    "launch-cmd": "tuxpaint",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "http://www.tuxpaint.org/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "methods": ["apt"],
    "apt": {
        "default": {
            "main-package": "tuxpaint",
            "install-packages": [
                "tuxpaint"
            ],
            "remove-packages": [
                "tuxpaint",
                "tuxpaint-data",
                "tuxpaint-config",
                "tuxpaint-plugins-default",
                "tuxpaint-stamps-default"
            ],
            "source": "universe"
        }
    },
    "snap": {
		"name": null
	}
}
