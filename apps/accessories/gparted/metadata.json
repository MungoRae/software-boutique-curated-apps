{
    "listed": true,
    "name": "GParted",
    "summary": "An advanced tool for editing and formatting partitions on disks.",
    "developer-name": "GParted Team",
    "developer-url": "http://gparted.org/contact.php",
    "description": "An advanced partition editor that uses `libparted` to detect and manipulate devices and partition tables while several (optional) filesystem tools provide support for filesystems not included in libparted.",
    "tags": "Partitioning Tools, Disks, Filesystems, Storage, Formatting",
    "launch-cmd": "pkexec gparted",
    "proprietary": false,
    "alternate-to": null,
    "urls": {
        "info": "http://gparted.org/",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "gparted",
            "install-packages": [
                "gparted",
                "xfsprogs",
                "reiserfsprogs",
                "reiser4progs",
                "jfsutils",
                "ntfs-3g",
                "dosfstools",
                "mtools",
                "kpartx",
                "dmraid",
                "dmsetup",
                "gpart"
            ],
            "remove-packages": [
                "gparted"
            ],
            "source": "main"
        }
    }
}
