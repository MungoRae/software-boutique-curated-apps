{
    "listed": true,
    "name": "Gnote",
    "summary": "Simple note taking application.",
    "developer-name": "GNOME Team",
    "developer-url": "https://git.gnome.org/browse/gnote/tree/AUTHORS",
    "description": "A desktop note-taking application which is simple and easy to use. It lets you organise your notes with Wiki style interconnects. ",
    "launch-cmd": "gnote",
    "proprietary": false,
    "alternate-to": "Post-it &reg; notes",
    "urls": {
        "info": "https://wiki.gnome.org/Apps/Gnote",
        "android-app": null,
        "ios-app": null
    },
    "arch": [
        "i386",
        "amd64",
        "armhf",
        "arm64"
    ],
    "releases": [
        "xenial",
        "zesty",
        "artful"
    ],
    "method": "apt",
    "installation": {
        "all": {
            "main-package": "gnote",
            "install-packages": [
                "gnote"
            ],
            "remove-packages": [
                "gnote"
            ],
            "source": "universe"
        }
    }
}
